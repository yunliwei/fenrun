<%= render 'layerbegin' %>
<div class="ibox-content form-horizontal m-t">
  <h3>新增类型</h3>
  <%= form_for(@busines,:html => {:class=>'form-horizontal m-t',:multipart => true}) do |f| %>
      <br>
      <br>
      <div class="form-group">
        <label class="col-sm-5 control-label">类型：</label>
        <div class="col-sm-3">
          <%= f.text_field :name,class:"form-control" %>
              </div>
              </div>
              <div class="form-group">
                <label class="col-sm-5 control-label">销量：</label>
                <div class="col-sm-3">
                  <%= f.text_field :salessum,class:"form-control" %>
                      </div>
                      </div>
                      <div class="form-group">
                        <label class="col-sm-5 control-label">联系电话：</label>
                        <div class="col-sm-3">
                          <%= f.text_field :phonenumber,class:"form-control" %>
                              </div>
                              </div>

                              <div class="form-group">
                                <label class="col-sm-5 control-label">工商执照：</label>
                                <div class="col-sm-3">
                                  <%= f.file_field  :businelicense,class:"form-control" %>
                                      </div>
                                      </div>
                                      <div class="form-group">
                                        <label class="col-sm-5 control-label">商家logo：</label>
                                        <div class="col-sm-3">
                                          <%= f.file_field  :businelogo,class:"form-control" %>
                                              </div>
                                              </div>
                                              <%= f.submit '确定',class: "btn btn-w-m btn-success mylink pull-right",id:'smt' %>
                                              <% end %>
                                              </div>
                                              <%= render 'layerend' %>
